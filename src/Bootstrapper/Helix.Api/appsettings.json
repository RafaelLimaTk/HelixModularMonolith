{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Mongo": {
    "Chat": {
      "ConnectionString": "mongodb://localhost:27017",
      "Database": "helix_chat"
    }
  },
  "Postgres": {
    "Chat": {
      "ConnectionString": "Host=localhost;Database=helix;Username=;Password="
    }
  },
  "Outbox": {
    "BatchSize": 100,
    "EmptyQueueDelaySeconds": 5,
    "PartialBatchDelaySeconds": 1,
    "ErrorRetryDelaySeconds": 10,
    "MessageTimeoutSeconds": 30,
    "EnableParallelProcessing": true,
    "MaxDegreeOfParallelism": 10
  }
}
